<jboss:ejb-jar xmlns:jboss="http://www.jboss.com/xml/ns/javaee" xmlns="http://java.sun.com/xml/ns/javaee"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:c="urn:clustering:1.0" xsi:schemaLocation="http://www.jboss.com/xml/ns/javaee
               http://www.jboss.org/j2ee/schema/jboss-ejb3-2_0.xsd
               http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
               version="3.1" impl-version="2.0">
    <enterprise-beans>
        <session>
            <ejb-name>ProcessServiceEJBImpl</ejb-name>
        </session>
        <session>
            <ejb-name>UserTaskServiceEJBImpl</ejb-name>
        </session>
    </enterprise-beans>
    <assembly-descriptor>
        <c:clustering>
            <ejb-name>ProcessServiceEJBImpl</ejb-name>
            <c:clustered>true</c:clustered>
        </c:clustering>
        <c:clustering>
            <ejb-name>UserTaskServiceEJBImpl</ejb-name>
            <c:clustered>true</c:clustered>
        </c:clustering>
    </assembly-descriptor>
</jboss:ejb-jar>
